{
  "name": "flex-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy:dev": "firebase use dev && ng build --aot && firebase deploy --only hosting",
    "deploy:prod": "firebase use prod && ng build --prod && firebase deploy --only hosting"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.2.6",
    "@angular/cdk": "11.2.4",
    "@angular/common": "11.2.6",
    "@angular/compiler": "11.2.6",
    "@angular/core": "11.2.6",
    "@angular/fire": "6.1.4",
    "@angular/forms": "11.2.6",
    "@angular/localize": "11.2.6",
    "@angular/platform-browser": "11.2.6",
    "@angular/platform-browser-dynamic": "11.2.6",
    "@angular/router": "11.2.6",
    "@ng-bootstrap/ng-bootstrap": "9.0.2",
    "bootstrap": "5.0.0-beta2",
    "rxjs": "6.6.6",
    "tslib": "2.1.0",
    "zone.js": "0.11.4",
    "firebase": "8.3.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1102.5",
    "@angular-devkit/build-angular": "0.1102.5",
    "@angular/cli": "11.2.5",
    "@angular/compiler-cli": "11.2.5",
    "@types/jasmine": "3.6.9",
    "@types/jest": "26.0.20",
    "@types/node": "12.20.6",
    "codelyzer": "6.0.1",
    "firebase-tools": "9.6.1",
    "fuzzy": "0.1.3",
    "husky": "5.1.3",
    "inquirer": "8.0.0",
    "inquirer-autocomplete-prompt": "1.3.0",
    "jasmine-core": "3.7.1",
    "jasmine-spec-reporter": "6.0.0",
    "jest": "26.6.3",
    "jest-preset-angular": "8.4.0",
    "karma": "6.2.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage": "2.0.3",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.5.4",
    "open": "8.0.3",
    "postcss": "8.2.8",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "protractor": "7.0.0",
    "ts-node": "9.1.1",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && ng lint && ng test",
      "pre-push": "ng build --aot true"
    }
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$"
      }
    }
  }
}
